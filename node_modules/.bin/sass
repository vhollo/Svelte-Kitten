#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/sass@1.32.12/node_modules/sass/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/sass@1.32.12/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules:/Users/sonya/Documents/DevProjects/node_modules:/Users/sonya/Documents/node_modules:/Users/sonya/node_modules:/Users/node_modules:/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/sass/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sass/sass.js" "$@"
else
  exec node  "$basedir/../sass/sass.js" "$@"
fi
