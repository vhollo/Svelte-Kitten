#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/esbuild@0.9.7/node_modules/esbuild/bin/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/esbuild@0.9.7/node_modules/esbuild/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/esbuild@0.9.7/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules:/Users/sonya/Documents/DevProjects/node_modules:/Users/sonya/Documents/node_modules:/Users/sonya/node_modules:/Users/node_modules:/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/node_modules/esbuild/bin/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/node_modules/esbuild/node_modules"
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
