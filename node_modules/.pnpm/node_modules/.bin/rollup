#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/rollup@2.47.0/node_modules/rollup/dist/bin/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/rollup@2.47.0/node_modules/rollup/dist/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/rollup@2.47.0/node_modules/rollup/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/rollup@2.47.0/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules:/Users/sonya/Documents/DevProjects/node_modules:/Users/sonya/Documents/node_modules:/Users/sonya/node_modules:/Users/node_modules:/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/node_modules/rollup/dist/bin/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/node_modules/rollup/dist/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/node_modules/rollup/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../rollup/dist/bin/rollup" "$@"
fi
