#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/vite@2.2.4/node_modules/vite/bin/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/vite@2.2.4/node_modules/vite/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/vite@2.2.4/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules:/Users/sonya/Documents/DevProjects/node_modules:/Users/sonya/Documents/node_modules:/Users/sonya/node_modules:/Users/node_modules:/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.100_svelte@3.38.2+vite@2.2.4/node_modules/@sveltejs/kit/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.100_svelte@3.38.2+vite@2.2.4/node_modules/@sveltejs/node_modules:/Users/sonya/Documents/DevProjects/template/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.100_svelte@3.38.2+vite@2.2.4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@2.2.4/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@2.2.4/node_modules/vite/bin/vite.js" "$@"
fi
